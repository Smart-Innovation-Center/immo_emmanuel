{% extends "structure/bars.html.twig" %}


{% block title %}
	{{ 'Bien des propri√©taires'|trans }}
{% endblock %}

{% block contenpage %}
	<div class="card mb-3">

		<div class="card-body">
			<h1>Voici le(s) agent(s) de l'agence
			</h1>

			<div class="table-responsive">
				<table class="table" style="background-color: #FFFFFF;">
					<thead class="thead">
						<tr>
							<th class="hidden-sm">Nom d'utilisateur</th>
							<th class="hidden-sm">Email</th>
							{#<th class="hidden-sm">Role</th>#}
						</tr>
					</thead>
					<tbody>

						{% for agent in agents %}
							<tr>
								<td class="hidden-sm">{{ agent.username }}</td>
								<td class="hidden-sm">{{ agent.email }}</td>
								{#<td class="hidden-sm">{{ agent.roles }}</td>#}
							</tr>
						</td>
					</tbody>
				</tr>

			{% else %}
				<div class="well">Nothing</div>
			{% endfor %}

		</tbody>
	</table>
</div></div></div>{% endblock %}
